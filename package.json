    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "^4.3.1",
    "@vue/composition-api": "^1.0.0-rc.6",
    "@vuepress/plugin-back-to-top": "^1.8.2",
    "@vuepress/plugin-pwa": "^1.8.2",
