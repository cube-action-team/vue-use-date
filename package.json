    "vuepress-plugin-element-tabs": "^0.2.8"
  },
  "bundlesize": [
    {
      "path": "./dist/*.min.js",
      "maxSize": "10 kB"
    }
  ],
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "homepage": "https://vue-use-date.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-action-team/vue-use-date.git"
  },
  "lint-staged": {
    "*.{js,vue,json}": [
