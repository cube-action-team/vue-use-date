    "babel-eslint": "^10.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "bundlesize": "^0.18.0",
    "chalk": "^4.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "dayjs": "^1.10.5",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "filesize": "^6.0.1",
    "flush-promises": "^1.0.2",
